{
    "name": "paylivre/laravel-csv-response",
    "license": "MIT",
    "description": "Add a CSV response type to Laravel",
    "require": {
        "php": "^8.1",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    }, 
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
         "psr-4": {
            "Paylivre\\CsvResponse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paylivre\\CsvResponse\\ResponseFactoryServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
